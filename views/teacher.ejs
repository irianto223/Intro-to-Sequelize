<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>teacher page</title>
  </head>
  <body>
    <table border="1px" width="100%">
      <tr>
        <th>id</th>
        <th>first name</th>
        <th>last name</th>
        <th>email</th>
      </tr>
      <% dataTeacher.forEach(d => { %>
      <tr>
        <td><%= d.id %></td>
        <td><%= d.first_name %></td>
        <td><%= d.last_name %></td>
        <td><%= d.email %></td>
      </tr>
      <% }) %>
    </table>
    <br><a href="/">back</a>
  </body>
</html>
